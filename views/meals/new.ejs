<%- include('../partials/header') %>
        
<form id="add-meal" action="/plans/<%= planId %>/meals" method="POST">
    <label>Meal:</label>
    <input type="string" name="seat" required pattern="[A-F][1-9]\d?">
    <label>Price:</label>
    <span>$</span><input type="number" name="price">
    <input type="submit" value="Add Meal">
</form>

